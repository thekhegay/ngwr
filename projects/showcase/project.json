{
  "name": "showcase",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "projects/showcase/src",
  "projectType": "application",
  "prefix": "ngwr",
  "schematics": {},
  "targets": {
    "build": {
      "executor": "@nx/angular:browser-esbuild",
      "options": {
        "outputPath": "dist/showcase",
        "index": "projects/showcase/src/index.html",
        "main": "projects/showcase/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "projects/showcase/tsconfig.app.json",
        "allowedCommonJsDependencies": ["highlight.js", "lodash"],
        "assets": [
          "projects/showcase/src/assets",
          "projects/showcase/src/favicon.ico",
          "projects/showcase/src/apple-touch-icon.png",
          "projects/showcase/src/icon.svg",
          "projects/showcase/src/icon-192.png",
          "projects/showcase/src/icon-512.png",
          "projects/showcase/src/manifest.webmanifest",
          "projects/showcase/src/robots.txt"
        ],
        "inlineStyleLanguage": "scss",
        "styles": ["projects/showcase/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "5kb",
              "maximumError": "10kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "projects/showcase/src/environments/environment.ts",
              "with": "projects/showcase/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "namedChunks": false,
          "extractLicenses": false,
          "sourceMap": false
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "showcase:build:production"
        },
        "development": {
          "buildTarget": "showcase:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "showcase:build"
      }
    }
  }
}
